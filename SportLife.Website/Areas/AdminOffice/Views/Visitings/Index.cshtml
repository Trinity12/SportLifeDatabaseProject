@model IEnumerable<SportLife.Core.Database.Visiting>

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/AdminOffice/Views/AdminHome/Index.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.VisitingDateTime)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Client.ClientId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Shedule.SheduleDay)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.VisitingDateTime)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Client.ClientId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Shedule.SheduleDay)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.VisitingId }) |
            @Html.ActionLink("Details", "Details", new { id=item.VisitingId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.VisitingId })
        </td>
    </tr>
}

</table>
